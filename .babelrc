{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1% in CN", "not ie <= 8"]
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-vue-jsx",
    ["import", {
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }]
  ]
}
